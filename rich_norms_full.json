        {
          "id": "N::0003",
          "statement_text": "Abrirá en el sentido de la evacuación toda puerta de salida: a) prevista para el paso de más de 200 personas en edificios de uso Residencial Vivienda o de 100 personas en los demás casos, o bien. b) prevista para más de 50 ocupantes del recinto o espacio en el que esté situada.",
          "obligation_type": "MANDATORY",
          "paragraph_number": 3,
          "applies_if": "(BUILDING.USAGE == 'RESIDENCIAL.VIVIENDA' AND EVACUATION.PERSONS > 200) OR (BUILDING.USAGE != 'RESIDENCIAL.VIVIENDA' AND EVACUATION.PERSONS > 100) OR EVACUATION.OCCUPANTS > 50",
          "satisfied_if": "DOOR.OPENING.DIRECTION == 'EVACUATION'",
          "exempt_if": null,
