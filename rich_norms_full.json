        {
          "id": "N::0005",
          "statement_text": "La anchura útil de este tipo de puertas y de las de giro automático después de su abatimiento, debe estar dimensionada para la evacuación total prevista.",
          "obligation_type": "MANDATORY",
          "paragraph_number": 4,
          "applies_if": "DOOR.TYPE == 'REVOLVING' OR DOOR.TYPE == 'AUTOMATIC.SWING'",
          "satisfied_if": "DOOR.WIDTH.USEFUL >= EVACUATION.TOTAL_EXPECTED",
          "exempt_if": null,
